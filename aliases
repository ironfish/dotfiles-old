# Navigation shortcuts.
alias ~="cd ~"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Listing shortcuts.
alias ll='ls -hl'
alias la='ls -al'

# Application shortcuts.
alias sp='subl --project "$@"'
alias v='open -a VimR'
# Directory shortcuts.
alias gww='cd ~/workspace/ww'

